{
  "name": "@robertjanes/font-data",
  "version": "0.2.2",
  "description": "Useful data concerning fonts, OpenType features, Unicode, PostScript, WOFF etc.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup lib/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "lint": "tsc"
  },
  "author": "Robert Janes",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "prettier": "^3.3.1",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  }
}
